    <!-- http://www.uagent.com/ontology#Action -->

    <owl:Class rdf:about="http://www.uagent.com/ontology#Action">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.uagent.com/ontology#triggers"/>
