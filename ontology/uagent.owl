                <owl:onClass rdf:resource="http://www.uagent.com/ontology#TransitionDescription"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.uagent.com/ontology#ofActionType"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.uagent.com/ontology#ActionType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


